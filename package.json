{
  "name": "@nuls/nuls-js",
  "version": "1.0.0-alpha.1",
  "description": "Isomorphic javascript SDK for Nuls blockchain",
  "main": "dist/nuls-js.cjs.js",
  "unpkg": "dist/nuls-js.umd.js",
  "jsdelivr": "dist/nuls-js.umd.js",
  "browser": "dist/nuls-js.umd.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist/**/*.{d.ts,js}"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:dev": "npm run clean && webpack --mode development --config webpack.config.js",
    "build:prod": "npm run clean && webpack --mode production --config webpack.config.js",
    "tsnode": "ts-node -O '{\"module\": \"commonjs\", \"allowJs\": true}' tests/index.ts",
    "test": "npm run jest && npm run lint",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "jest": "jest",
    "lint": "./node_modules/.bin/eslint --ext=js,ts src/ testConfigs/ testConfigs/",
    "docs:clean": "rimraf typedocs",
    "docs": "npm run docs:clean && typedoc --options ./typedoc.js ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amalcaraz/nuls-js.git"
  },
  "keywords": [
    "nuls",
    "sdk"
  ],
  "author": "amalcaraz <amalcaraz89@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "private": true,
    "registry": "http://my-internal-registry.local"
  },
  "engines": {
    "node": ">=7.8.0"
  },
  "peerDependencies": {
    "es6-promise": "4.2.x"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@types/deepmerge": "^1.3.3",
    "@types/jest": "^23.1.0",
    "@types/node": "^9.6.32",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "config": "^1.30.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.23.0",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.6.0",
    "js-yaml": "^3.11.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.0.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.1",
    "typescript-eslint-parser": "^19.0.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^2.0.10",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "deepmerge": "^2.1.0"
  }
}
