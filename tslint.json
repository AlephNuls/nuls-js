{
	"extends": "tslint:recommended",
	"rules": {
		"indent": false,
		"whitespace": false,
		"one-line": false,
		"curly": false,
		"trailing-comma": false,
		"quotes": false,
		"quotemark": [true, "single"],
		"typedef": [false, "call-signature", "arrow-call-signature", "parameter", "arrow-parameter", "property-declaration", "variable-declaration", "member-variable-declaration", "object-destructuring", "array-destructuring"],
		"max-line-length": [false],
		"no-bitwise": false,
		"no-console": [false],
		"one-variable-per-declaration": false,
		"no-var-requires": false,
		"object-literal-sort-keys": [false],
	}
}
